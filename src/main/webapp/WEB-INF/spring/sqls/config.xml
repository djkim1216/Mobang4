		<mapper resource="/mybatis/agent/notice-mapper.xml"/>
		<mapper resource="/mybatis/user/roomsearch-mapper.xml"/>
		<mapper resource="/mybatis/user/favorite-mapper.xml"/>
		<mapper resource="/mybatis/user/review-mapper.xml"/>
		<mapper resource="/mybatis/user/search-mapper.xml"/>
		<mapper resource="/mybatis/common/room-mapper.xml"/>
	</mappers>
</configuration>
