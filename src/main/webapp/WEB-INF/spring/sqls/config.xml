<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<typeAliases>
		<typeAlias type="com.finalproject.mobang.admin.dto.AccountBoardDto" alias="accountBoardDto"/>
		<typeAlias type="com.finalproject.mobang.admin.dto.QnaBoardDto" alias="qnaBoardDto"/>
		<typeAlias type="com.finalproject.mobang.user.dto.ReviewDto" alias="reviewDto"/>
		<typeAlias type="com.finalproject.mobang.user.dto.roomsearchDto" alias="roomsearchDto"/>
		<typeAlias type="com.finalproject.mobang.agent.dto.AgentNoticeDto" alias="agentNoticeDto"/>
		<typeAlias type="com.finalproject.mobang.user.dto.FavoriteDto" alias="favoriteDto"/>
		<typeAlias type="com.finalproject.mobang.user.dto.SearchDto" alias="searchDto"/>
		<typeAlias type="com.finalproject.mobang.common.dto.UploadFile" alias="uploadFile"/>
		<typeAlias type="com.finalproject.mobang.common.dto.RoomDto" alias="roomDto"/>
	</typeAliases>
	
	<!-- resource : src/main/resources부터 검색 -->
	<mappers>
		<mapper resource="/mybatis/admin/account-mapper.xml"/>
		<mapper resource="/mybatis/admin/qna-mapper.xml"/>
